#mysql -D $CONFIGDTSD_MYSQLDB -u $CONFIGDTSD_MYSQLUSER -p$CONFIGDTSD_MYSQLPASS -e "INSERT INTO COLLECTINFODB (COLLECTVARIABLE, COLLECTVALUE) VALUES (\"CURRENTSECTOR\",\"$CURRENTSECTOR\")"

